{
    "name": "chiefgroup/laravel-clue",
    "type": "library",
    "license": "MIT",
    "description": "clue 项目 RPC 封装",
    "authors": [
        {
            "name": "observer",
            "email": "sxpd7788@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "hyperf/jet": "^0.6.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^8.0 || ^9.0",
        "orchestra/testbench": "^6.0 || ^7.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChiefGroup\\LaravelClue\\ServiceProvider"
            ],
            "aliases": {
                "YiLiaoService": "ChiefGroup\\LaravelClue\\YiLiaoServiceFacade",
                "FeiYu": "ChiefGroup\\LaravelClue\\FeiYuFacade",
                "FormClue": "ChiefGroup\\LaravelClue\\FormClueFacade",
                "Filter": "ChiefGroup\\LaravelClue\\FilterFacade",
                "Tag": "ChiefGroup\\LaravelClue\\TagFacade",
                "Sheet": "ChiefGroup\\LaravelClue\\SheetFacade",
                "OceanEngine": "ChiefGroup\\LaravelClue\\OceanEngineFacade",
                "OpenDouYin": "ChiefGroup\\LaravelClue\\OpenDouYinFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "ChiefGroup\\LaravelClue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChiefGroup\\LaravelClue\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
